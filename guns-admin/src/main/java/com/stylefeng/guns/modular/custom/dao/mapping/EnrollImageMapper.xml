<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.stylefeng.guns.modular.custom.dao.EnrollImageMapper">

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="com.stylefeng.guns.modular.custom.model.EnrollImage">
        <id column="imageid" property="imageid" />
        <result column="enrollid" property="enrollid" />
        <result column="file_key" property="fileKey" />
        <result column="file_path" property="filePath" />
        <result column="create_date" property="createDate" />
    </resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        imageid, enrollid, file_key AS fileKey, file_path AS filePath, create_date AS createDate
    </sql>

</mapper>
